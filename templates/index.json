{
  "layout": "theme.prelaunch",
  "sections": {
    "header": {
      "type": "header",
      "settings": {
        "logo_text": "AAEL",
        "collection_text": "C24",
        "right_button_text": "Let me know",
        "right_button_link": "#newsletter-form"
      }
    },
    "hero": {
      "type": "hero-lead",
      "settings": {
        "hero_text": "AAEL creates limited series of interior artifacts primarily out of metal.",
        "max_width": 800,
        "padding_top": 40,
        "padding_bottom": 64
      }
    },
    "product": {
      "type": "product-carousel",
      "blocks": {
        "slide-1": {
          "type": "slide",
          "settings": {
            "image_light_url": "https://i.ibb.co/YFqGLMGM/amo-5-d.webp",
            "image_dark_url": "https://i.ibb.co/x8CZ1cNY/amo-5-n.webp",
            "alt_text": "AMO Desk Lamp"
          }
        },
        "slide-2": {
          "type": "slide",
          "settings": {
            "image_light_url": "https://i.ibb.co/M5N8XTHW/amo-4-d.webp",
            "image_dark_url": "https://i.ibb.co/PGdjc1mn/amo-4-n.webp",
            "alt_text": "AMO Desk Lamp"
          }
        },
        "slide-3": {
          "type": "slide",
          "settings": {
            "image_light_url": "https://i.ibb.co/VWjW5STB/d-v3.webp",
            "image_dark_url": "https://i.ibb.co/fmHHdcb/n-v3.webp",
            "alt_text": "AMO Desk Lamp"
          }
        }
      },
      "block_order": ["slide-1", "slide-2", "slide-3"],
      "settings": {
        "variant": "prelaunch",
        "max_width": 1440,
        "padding_bottom": 80
      }
    },
    "newsletter_footer": {
      "type": "newsletter-footer",
      "blocks": {
        "link-1": {
          "type": "link",
          "settings": {
            "label": "ABOUT",
            "link": "/pages/about"
          }
        },
        "link-2": {
          "type": "link",
          "settings": {
            "label": "INSTA",
            "link": "https://instagram.com",
            "open_new_tab": true
          }
        }
      },
      "block_order": ["link-1", "link-2"],
      "settings": {
        "newsletter_title": "Let me know when AMO is available",
        "padding_bottom": 120
      }
    }
  },
  "order": [
    "header",
    "hero",
    "product",
    "newsletter_footer"
  ]
}
